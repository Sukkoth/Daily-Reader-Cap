{
  "name": "capacitor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "sync": "tsc -b && vite build && npx cap sync",
    "ionic:build": "tsc -b && vite build",
    "ionic:serve": "vite dev --host",
    "get-ip": "node scripts/get-ip.js",
    "get-device-id": "node scripts/get-adb-device.js",
    "dev:reload": "HOST=$(npm run get-ip --silent) && TARGET=$(npm run get-device-id --silent) && npx npm run ionic:build && npx cap copy android && npx cap run android --target=$TARGET --live-reload --no-sync --port=5173 --host=$HOST & npx npm run ionic:serve --no-open --host=$HOST --port=5173"
  },
  "dependencies": {
    "@capacitor/android": "^7.2.0",
    "@capacitor/app": "^7.0.1",
    "@capacitor/cli": "^7.2.0",
    "@capacitor/core": "^7.2.0",
    "@capacitor/preferences": "^7.0.1",
    "@capacitor/share": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@tailwindcss/vite": "^4.1.8",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "jotai": "^2.12.5",
    "lucide-react": "^0.511.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-modal-sheet": "^4.4.0",
    "react-router-dom": "^6.30.1",
    "react-swipeable": "^7.0.2",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-terminal": "^1.3.0"
  }
}
