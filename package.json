{
  "name": "capacitor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "sync": "tsc -b && vite build && npx cap sync",
    "ionic:build": "tsc -b && vite build",
    "ionic:serve": "vite dev --host",
