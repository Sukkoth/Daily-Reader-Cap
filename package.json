    "@tailwindcss/vite": "^4.1.8",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.30.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.8"
  },
